#include <iostream>
using namespace std;
const int N=100;
 
//countNumberOfTimesTargetIsInDiagonal containing row, col
int countNumberOfTimesTargetIsInDiagonal(int n, int A[][N], int row, int col, int target){
	int ans=0;
	return ans;
}
 
int main() {
	int n;
	cin>>n;
	int A[N][N];
	for(int i=0;i<n;i++){
